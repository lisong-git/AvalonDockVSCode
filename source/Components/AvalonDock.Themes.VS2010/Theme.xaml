            Mode="OneWay"
            Path="IsEnabled"
            RelativeSource="{RelativeSource Self}" />
          <Binding Path="CanClose" />
        </MultiBinding>
      </MenuItem.Visibility>
    </MenuItem>
  </ContextMenu>

