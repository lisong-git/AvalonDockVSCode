              Width="24"
              Height="24"
              Data="M7.41,8.58L12,13.17L16.59,8.58L18,10L12,16L6,10L7.41,8.58Z"
              Fill="Blue"
              Opacity="0.38"
              RenderTransformOrigin="0.5 0.5">
              <!--<Path.RenderTransform>
                                <RotateTransform>
                                    <RotateTransform.Angle>
                                        -90
                                    </RotateTransform.Angle>
                                </RotateTransform>
                            </Path.RenderTransform>-->
