                      <Path
                        x:Name="PART_ImgPinMaximize"
                        Width="9"
                        Height="9"
                        VerticalAlignment="Center"
                        Data="{DynamicResource PinMaximize}"
                        Fill="{DynamicResource {x:Static reskeys:ResourceKeys.DocumentWellTabButtonSelectedInactiveGlyph}}"
